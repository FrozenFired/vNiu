if users && users.length > 0
	-a;
	h2 公司成员
	table.table.table-hover.table-bordered
		thead
			tr
				th 账户
				th 编号
				th 角色
		tbody
			each object in users
				-a;
				tr(class="object-id-"+object._id)
					td: a(href=userAct+"/"+object._id) #{object.code}
					td #{object.cd}
					td #{Conf.wsRole[object.role]}