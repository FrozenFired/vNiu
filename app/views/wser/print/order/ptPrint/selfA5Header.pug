.row
	.col-sm-4
		h4 地址: #{group.addr}
		div(style="height:5px")
		h4 电话: #{group.tel}
	.col-sm-4
		- let cterCp = new Object();
		if object.cter
			- cterCp = object.cter;
		h4 客户: #{cterCp.nome}
		div(style="height:5px")
		h4 电话: #{cterCp.tel}
	.col-sm-4
		h5 No: #{object.code}
		h5 Date: #{moment(object.ctAt).format('DD/MM/YYYY')}
div(style="height:20px")